  <source-file src="src/android/service/DocSuperAndSub.java" target-dir="src/cn/com/ths/exportdoc/thsexportdoc/service" />
  <source-file src="src/android/service/DocTable.java" target-dir="src/cn/com/ths/exportdoc/thsexportdoc/service" />
  <source-file src="src/android/service/DocumentPage.java" target-dir="src/cn/com/ths/exportdoc/thsexportdoc/service" />
