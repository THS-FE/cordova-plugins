<?xml version='1.0' encoding='utf-8'?>
<plugin id="cn.com.ths.exportdoc" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
<name>thsExportDoc</name>
 <js-module name="thsExportDoc" src="www/thsExportDoc.js">
    <clobbers target="cordova.plugins.thsExportDoc" />
 </js-module>
<platform name="android">
 <config-file parent="/*" target="res/xml/config.xml">
   <feature name="thsExportDoc"><param name="android-package" value="cn.com.ths.exportdoc.thsexportdoc.thsExportDoc" /></feature>
 </config-file>
 <config-file parent="/*" target="AndroidManifest.xml"></config-file>
 <config-file target="AndroidManifest.xml" parent="/*">
            <!-- 写入扩展存储，向扩展卡写入数据，用于写入离线定位数据-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
  </config-file>
 <source-file src="src/android/thsExportDoc.java" target-dir="src/cn/com/ths/exportdoc/thsexportdoc" />
 <source-file src="src/android/enums/DocParamTypeEnum.java" target-dir="src/cn/com/ths/exportdoc/thsexportdoc/enums" />
