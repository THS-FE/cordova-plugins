 <source-file src="src/android/service/DocComplexTable.java" target-dir="src/cn/com/ths/exportdoc/thsexportdoc/service" />
  <source-file src="src/android/service/DocDynamicTable.java" target-dir="src/cn/com/ths/exportdoc/thsexportdoc/service" />
  <source-file src="src/android/service/DocImage.java" target-dir="src/cn/com/ths/exportdoc/thsexportdoc/service" />
